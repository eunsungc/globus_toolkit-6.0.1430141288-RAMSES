prefix=/usr/local/globus-6
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: globus-io
Description: Globus Toolkit - Uniform I/O interface to stream and datagram style communications
Version: 11.3
Requires.private: globus-common >= 14, globus-xio >= 3, globus-gss-assist >= 8, globus-gssapi-gsi >= 10, globus-xio-gsi-driver >= 2, globus-gssapi-error >= 4
Libs: -L${libdir} -lglobus_io
Cflags: -I${includedir}
